   if (poids >= 20):
      print (40)
   
   else:
      print(30)

//niveau 1-6 Protection du village

nbMaisons = int(input())

xMin = 1000000
yMin = 1000000
xMax = 0
yMax = 0

for maison in range(nbMaisons):
    x = int(input())
    y = int(input())

    if (x < xMin):
        xMin = x 
    if (x > xMax):
        xMax = x

    if (y < yMin):
        yMin = y
    if (y > yMax):
        yMax = y

print(2 * (xMax - xMin + yMax - yMin))

//niveau 1-6 Le juste prix

nbMarchands = int(input())
prixMin= int(input())
pos= 1

for posMarchand in range(2, nbMarchands +1 ):
   prixGalettes = int(input())
   if (prixGalettes <= prixMin):
      prixMin = prixGalettes
      pos = posMarchand

print (pos)   

//niveau 1-7: Espion etranger

Debut = int(input())
Fin=int(input())
nbEntrees=int(input())

espion=0

for i in range(nbEntrees):
   dateEntree=int(input())
   if (dateEntree>=Debut and dateEntree<=Fin):
      espion += 1

print (espion)  

//niveau 1-7 Maison de l'espion
xMin = int(input())
xMax = int(input())

yMin = int(input())
yMax = int(input())

nbMaisons = int(input())

nbMaisonsDansZone = 0

for i in range(nbMaisons):
    x = int(input())
    y = int(input())

    if (x >= xMin and x <= xMax and y >= yMin and y <= yMax):
        nbMaisonsDansZone += 1

print(nbMaisonsDansZone)

// niveau 1-7 Nombre de jours dans le mois:
nbMois=int(input())

for i in range (nbMois):
   totalJour=0
   
   if (nbMois <=3):
      totalJour=30
   if (nbMois >= 4 and nbMois <=6):
      totalJour=31
   if (nbMois>=7 and nbMois <=9):
      totalJour=30
   if (nbMois==10):
      totalJour=31
   if (nbMois==11):
      totalJour=29
      
print(totalJour)

//niveau 1-7 Amitié entre garde:
debut1=int(input())
fin1=int(input())
debut2=int(input())
fin2=int(input())

if (fin1 < debut2 or fin2 < debut1):
   print("Pas amis")
else:
   print ("Amis")

// niveau 1-7: Nombre de persnnes à la fête
nbPersonnes=int(input())
nbPersMax=0
nbPersSim=0

for i in range (nbPersonnes*2):
   n=int(input())
   if (n > 0):
      nbPersSim += 1
   else:
      nbPersSim -= 1
      
   if (nbPersSim > nbPersMax):
      nbPersMax = nbPersSim
      
print (nbPersMax)

#Niveau 1-7 Casernes de pompiers:

nbPaires=int(input())

for paire in range (nbPaires):
   xMin1=int(input())
   xMax1=int(input())
   yMin1=int(input())
   yMax1=int(input())
   
   xMin2=int(input())
   xMax2=int(input())
   yMin2=int(input())
   yMax2=int(input())
   
   if(not(xMax1 <= xMin2 or xMax2 <= xMin1 ) and not (yMax1 <= yMin2 or yMax2 <= yMin1)):
      print("OUI")
   
   else:
      print("NON")
      
 # niveau 1-7 Personne disparue:
 
 numPers=int(input())
tailleListe=int(input())

dansLaVille = True 

for i in range (tailleListe):
   num=int(input())
   
   if (num==numPers):
      dansLaVille = False
      
if (dansLaVille):
   print("Encore dans la ville")
   
else:
   print("Sorti de la ville")
   
   #niveau 1-7 La grande fête
   
   dateDebutEtude= int(input())
dateFinEtude= int(input())

nbInvites=int(input())

espion=0

for i in range (nbInvites):
   dateDebutInvite= int(input())
   dateFinInvite= int(input())
   
   if (not(dateFinInvite < dateDebutEtude or dateFinEtude < dateDebutInvite)):
         espion += 1

print (espion)


# Niveau 1-7: 
nbSuspects=int (input())

for i in range (nbSuspects):
   taille=int(input())
   age=int(input())
   poids=int(input())
   cheval=int(input())
   brun=int(input())
   
   criteria=0
   if (taille >=178 and taille <=182):
      criteria +=1
      
   if (age >= 34):
      criteria+=1
      
   if (poids < 70):
      criteria +=1
      
   if (cheval == 0):
      criteria +=1
      
   if (brun == 1):
      criteria +=1
      
   if (criteria == 0):
      print ("Impossible")
   
   if (criteria == 3 or criteria ==4):
      print ("Probable")
      
   if (criteria==5):
      print("Très probable")
      
   else:
      print("Peu probable")
