//niveau 1-6 Villes et Villages
nbLieux = int (input())

nbVilles = 0

for i in range(nbLieux):
   nbHabs = int(input())
   if (nbHabs > 10000):
      nbVilles +=1
      
print (nbVilles)

//niveau 1-6 Planning de la journée
nbDepart = int (input())

nbVilles = int(input())

nbVillesproches=0

for i in range(nbVilles):
   distances = int(input())
   diff = nbDepart - distances
   if (abs(diff)<=50):
      nbVillesproches +=1
      
print (nbVillesproches)

//niveau 1-6 distancel la plus longue
walkingDay = int(input())

distanCemax = 0

for i in range (walkingDay):
   distanCe = int(input())
   if (distanCemax < distanCe):
      distanCemax = distanCe
 
print (distanCemax)

//niveau 1-6 calcul denivele

nbDeniveles = int(input())

alMonte = 0
alDescente = 0

for i in range (nbDeniveles):
   varAltitude=int(input())
   if (varAltitude < 0):
      alDescente -= varAltitude
   else:
      alMonte += varAltitude

print (alMonte)
print (alDescente)

//niveau 1-6 type d'arbres

hauteur = int(input())
nbFolioles = int(input())


if hauteur <= 5 and nbFolioles >= 8:
   print ("Tinuviel")

if hauteur >=10 and nbFolioles >= 10:
   print ("Calaelen")
   
if hauteur <=8 and nbFolioles <= 5:
   print ("Falarion")

if hauteur >=12 and nbFolioles <= 7:
   print ("Dorthonion")

